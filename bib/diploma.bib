	author = {Edgar, Robert C},
	month = aug,
	year = {2004},
	note = {{PMID:} 15318951},
	keywords = {Cluster Analysis, Computational Biology, Phylogeny, Sequence Alignment, Software, Software Design, Time Factors},
	pages = {113}
},

