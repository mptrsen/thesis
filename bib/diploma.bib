	year = {2001},
	note = {{PMID:} 11266569},
	keywords = {Algorithms, Computational Biology, Databases, Factual, Information Storage and Retrieval, Sensitivity and Specificity, Sequence Alignment, Software},
	pages = {1647--1652},
	file = {Rognes2000.paralign.pdf:/home/mpetersen/bib/storage/W2BJDBHX/Rognes2000.paralign.pdf:application/pdf}
